{
  "name": "facebook-ui-clone",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "dependencies": {
    "clsx": "^2.1.0",
    "formik": "^2.2.9",
    "moment": "^2.29.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^3.11.0",
    "react-router-dom": "^6.4.3",
    "react-tooltip": "^5.26.3",
    "tailwind-merge": "^2.2.1",
    "yup": "^0.32.11"
  },
  "scripts": {
